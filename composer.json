{
    "name": "zendframework/zend-markup",
    "description": "provides an extensible way for parsing text and rendering lightweight markup languages like BBcode and Textile",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "markup"
    ],
    "homepage": "http://packages.zendframework.com/",
    "autoload": {
        "psr-0": {
            "ZendMarkup": "library/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-loader": "2.0.*@dev",
        "zendframework/zend-filter": "2.0.*@dev",
        "zendframework/zend-stdlib": "2.0.*@dev"
    }
}
